            <custom_style><![CDATA[.cms-index-index #maincontent {
    padding: 0;
}
.columns .column.main {
    padding: 0;
}
.owl-dots {
    margin-top: 10px;
}
.owl-theme .owl-dots .owl-dot span {
    width: 8px;
    height: 8px;
    background: transparent;
    border: 1px solid #000;
}
.owl-theme .owl-dots .owl-dot.active span, .owl-theme .owl-dots .owl-dot:hover span {
    background: #000;
}
.product-item-photo {
    padding: 0;
    border: 0;
    border-radius: 0;
}
.container {
    width: 100%;
}
.footer-top, .footer-middle, .footer-bottom {
    padding-left: 200px;
    padding-right: 100px;
}
.footer-bottom {
    padding-top: 0;
    padding-bottom: 0;
}
.footer-bottom > .container {
    border-top: 1px solid #0f0f0f;
    padding: 15px 0;
}
.footer-bottom address {
    float: none;
    text-align: center;
    font-size: 12px;
}
.footer-middle {
    padding-bottom: 25px;
    padding-top: 50px;
}
.footer-middle .block .block-title {
    margin-bottom: 30px;
}
.footer-middle .block .block-title strong {
    font-weight: 700;
    font-size: 14px;
    text-transform: uppercase;
}
.footer-middle .block .block-content {
    font-size: 14px;
}
@media (min-width: 768px){
    .footer-middle .row {
        display: flex;
    }
    .footer-middle .col-sm-3:first-child {
        border-left: 0;
        padding-left: 0;
        text-align: left;
    }
    .footer-middle .col-sm-3 {
        width: 20%;
        border-left: 1px solid #0f0f0f;
        padding: 10px 50px;
    }
    .footer-middle .col-sm-5 {
        border-left: 1px solid #0f0f0f;
        padding: 10px 50px;
    }
    body.layout-fullwidth .page-wrapper > .breadcrumbs .items {
        padding-left: 200px;
    }
    .page-title-wrapper {
        padding: 0 50px 0 180px;
    }
    .page.messages {
        padding: 0 50px 0 180px;
    }
}
@media (max-width:1199px){
    .footer-top, .footer-middle, .footer-bottom {
        padding-left: 150px;
        padding-right: 50px;
    }
    .footer-middle .col-sm-3, .footer-middle .col-sm-5 {
        padding: 10px 15px;
    }
}
@media (max-width:767px){
    .footer-top, .footer-middle, .footer-bottom, #maincontent {
        padding-left: 15px;
        padding-right: 15px;
    }
}

.footer .newsletter > label {
    display: none;
}
.footer input#newsletter {
    height: 60px;
    border-radius: 30px;
    background-color: transparent;
    line-height: 58px;
    padding: 0 30px;
    font-size: 14px;
    border-color: #999;
    float: left;
    max-width: 300px;
    color: #fff;
}
.footer .newsletter button.action.subscribe {
    height: 60px;
    font-size: 12px;
    font-weight: 700;
    text-transform: uppercase;
    float: left;
    margin-left: -60px;
    background: transparent;
    border: 0;
}]]></custom_style>
